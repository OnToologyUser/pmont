                    <rdf:Description rdf:about="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Publication"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:complementOf>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#isPublicationOf"/>
                                        <owl:someValuesFrom rdf:resource="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Member"/>
                                    </owl:Restriction>
                                </owl:complementOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#isPublicationOf"/>
                                <owl:someValuesFrom rdf:resource="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Person"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Publication"/>
