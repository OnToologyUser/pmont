#################################################################
#    Classes
#################################################################

###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Campaign
:Campaign rdf:type owl:Class ;
          rdfs:comment "[SuperClase] Campaña, define año de la elección, con fecha de inicio y fin de campaña. La cual se relaciona con las [Clases] PoliticalParty y Candidate" ;
          rdfs:label "Campaign" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Candidate
:Candidate rdf:type owl:Class ;
           rdfs:subClassOf :Member ;
           rdfs:comment "[SubClase] Candidato" ;
           rdfs:label "Candidate" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#CandidateCampaign
:CandidateCampaign rdf:type owl:Class ;
                   rdfs:comment "[Clase] Candidato para una campaña específica" ;
                   rdfs:label "CandidateCampaign" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#EligibleVoter
:EligibleVoter rdf:type owl:Class ;
               rdfs:subClassOf :Person ;
               rdfs:comment "[SubClase] Votante, contiene [individuos] ciudadanos en general mayores de edad, además la [SubClase] Miembros -> Candidato, almacena [individuos] de candidatos." ;
               rdfs:label "EligibleVoter" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#GeographicalImpact
:GeographicalImpact rdf:type owl:Class ;
                    rdfs:comment "[Clase] Describe el impacto de la publicación en tres [individuos] local, nacional e internacional." ;
                    rdfs:label "GeagraphicalImpact" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Ideology
:Ideology rdf:type owl:Class ;
          rdfs:comment "[Clase] Idiología política impacta en las [Clases] Political Party, ElegibleVoter y MassMedia" ;
          rdfs:label "Ideology" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#MassMedia
:MassMedia rdf:type owl:Class ;
           rdfs:subClassOf :Organization ;
           rdfs:comment "[SubClase] Medios de Comunicación" ;
           rdfs:label "MassMedia" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Member
:Member rdf:type owl:Class ;
        rdfs:subClassOf :EligibleVoter ;
        rdfs:label "Member" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#NewsItem
:NewsItem rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Publication
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isPublicationOf ;
                                                       owl:someValuesFrom :MassMedia
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Publication ;
          rdfs:comment "[SubClase] Noticia, publicación de la [SubClase] MassMedia" ;
          rdfs:label "NewsItem" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#NotEligibleVoter
:NotEligibleVoter rdf:type owl:Class ;
                  rdfs:subClassOf :Person ;
                  rdfs:comment "[SubClase] No votantes, contiene [individuos] ciudadanos en general menores de edad." ;
                  rdfs:label "NotEligibleVoter" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Opinion
:Opinion rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Publication
                                                    [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                             owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                owl:onProperty :isPublicationOf ;
                                                                                                owl:someValuesFrom :Member
                                                                                              ]
                                                                           ]
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :isPublicationOf ;
                                                                             owl:someValuesFrom :Person
                                                                           ]
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Publication ;
         rdfs:comment "[SubClase] Publicación \"Opinión\", está ligado a las opiniones abiertas de los ciudadanos, ya sea ElegibleVoter o NotElegibleVoter, por lo tal a la [Clase] Person" ;
         rdfs:label "Opinion" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Organization
:Organization rdf:type owl:Class ;
              owl:disjointUnionOf ( :MassMedia
                                    :PoliticalParty
                                  ) ;
              rdfs:comment "[SuperClase] Organización, contiene las [SubClases] MassMedia y PoliticalParty" ;
              rdfs:label "Organization" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Person
:Person rdf:type owl:Class ;
        owl:disjointUnionOf ( :EligibleVoter
                              :NotEligibleVoter
                            ) ;
        rdfs:comment "[SuperClase] Persona, contiene las [SubClases] No Votante y Votante -> Miembro -> Candidato" ;
        rdfs:label "Person" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Place
:Place rdf:type owl:Class ;
       rdfs:comment "[Clase] Lugares, describe las ubicaciones geográficas, ligadas a las [clases] publication, person, organization." ;
       rdfs:label "Place" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#PoliticalParty
:PoliticalParty rdf:type owl:Class ;
                rdfs:subClassOf :Organization ;
                rdfs:comment "[SubClase] Partido político" ;
                rdfs:label "PoliticalParty" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Proposal
:Proposal rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( [ owl:intersectionOf ( :Publication
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :isPublicationOf ;
                                                                       owl:someValuesFrom :PoliticalParty
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :isPublicationOf ;
                                                owl:someValuesFrom :Candidate
                                              ]
                                            )
                              ] ;
          rdfs:subClassOf :Publication ;
          rdfs:comment "[SubClase] Publicación \"Propuesta\", está ligada a la publicaciones de las [Clases] Candidate y PoliticalParty" ;
          rdfs:label "Proposal" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#Publication
:Publication rdf:type owl:Class ;
             owl:disjointUnionOf ( :NewsItem
                                   :Opinion
                                   :Proposal
                                 ) ;
             rdfs:comment "[SuperClase] Publicación, contiene los diferentes tipos en [SubClases] NewsItem, Opinion, Proposal" ;
             rdfs:label "Publication" .


###  https://hectorguedea.com/ontologia/Political-Marketing-Ontology.owl#PublicationPolarity
:PublicationPolarity rdf:type owl:Class ;
                     rdfs:comment "[Clase] Polaridad, contiene los [inviduos] Positive, Neutral, Negative" ;
                     rdfs:label "PublicationPolarity" .


